{
    "name": "f1monkey/eve-microservice-template",
    "description": "EVE microservice template repository",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "baldinof/roadrunner-bundle": "^1.2",
        "doctrine/annotations": "^1.10",
        "doctrine/collections": "^1.6",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "f1monkey/codeception-ext": "^1.0",
        "f1monkey/eve-esi-bundle": "^0.1.3",
        "f1monkey/monolog-extensions": "^1.0",
        "f1monkey/request-handle-bundle": "^1.0.2",
        "jms/serializer-bundle": "^3.6",
        "lexik/jwt-authentication-bundle": "^2.8",
        "nelmio/api-doc-bundle": "^3.6",
        "spiral/roadrunner": "^1.8",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/property-access": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/translation": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-cli": "^1.0",
        "codeception/module-db": "^1.0",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-rest": "^1.2",
        "codeception/module-symfony": "^1.1",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "phpstan/phpstan": "^0.12.29",
        "symfony/debug-bundle": "5.1.*",
        "symfony/maker-bundle": "^1.19"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "vendor/bin/codecept run",
        "phpstan": "vendor/bin/phpstan analyze src --level max"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
